<div class="page-header">
  <%= link_to notes_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_note_path(@note), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show note</h1>
</div>

<dl class="dl-horizontal">
  <dt>Subject:</dt>
  <dd><%= @note.subject %></dd>

  <dt>Body:</dt>
  <dd><%= @note.body %></dd>

  <dt>Created by:</dt>
  <dd><%= @note.created_by %></dd>

  <dt>Notable:</dt>
  <dd><%= @note.notable %></dd>

</dl>
